import java.math.BigDecimal;

public class StackReferenceBased {
	
	public static void main(String[] args) {
		Object v = new Object();
		v = 5.1555123;
		BigDecimal a = new BigDecimal(5.1);
		System.out.println((a instanceof BigDecimal));
		
	}
	
	private class Node {
		private Object data;
		private Node next;
		
		private Node() {
			
		}
	}
}
