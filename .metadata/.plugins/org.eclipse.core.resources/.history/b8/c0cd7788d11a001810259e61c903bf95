import java.util.Scanner;

public class Infix2PostfixCalculator {
	private static StackReferenceBased opStack;
	private static StackReferenceBased inStack;
	
	public static void main(String[] args) {
		opStack = new StackReferenceBased();
		inStack = new StackReferenceBased();
		
		String input = "(10 + 3 * 4 / 6)";
		String test = "abc";
		System.out.println(test);
		String test2 = test.substring(2);
		test = test.substring(0, 2);
		System.out.println(test + " " + test2 + " " + test.length());
	}
	
	private static String convertPostFix(String infix) {
		infix = infix.replaceAll("\\s", "");
		int search = infix.length() - 1;
		String curr = "";
		while (infix.length() != 0) {
			curr = infix.substring(search);
		}
		return "";
	}
	
	private static int getPostFix(String postfix) {
		return -1;
	}
}
